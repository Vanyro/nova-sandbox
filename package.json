{
  "name": "nova_sandbox",
  "version": "3.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "tsc --noEmit",
    "seed": "tsx src/seed/index.ts",
    "seed:reset": "tsx src/seed/index.ts --reset",
    "seed:light": "SEED_MODE=light tsx src/seed/index.ts",
    "seed:stress": "SEED_MODE=stress tsx src/seed/index.ts",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "test": "node --import tsx --test tests/*.test.ts",
    "test:phase3": "node --import tsx --test tests/phase3.test.ts",
    "simulation:start": "tsx -e \"import { startSimulation } from './src/worker/simulationEngine.js'; startSimulation();\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.0.3",
    "prettier": "^3.7.4",
    "prisma": "^6.19.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@fastify/env": "^5.0.3",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.19.1",
    "better-sqlite3": "^12.5.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "fastify-plugin": "^5.1.0",
    "tsx": "^4.21.0"
  }
}
